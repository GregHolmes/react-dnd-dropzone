{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended", "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "plugins": ["react"],
    "globals": {
        "GLOBALS": true,
        "Promise": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn"
    }
}
